<Page x:Class="ibcdatacsharp.UI.AngleGraph.AngleGraph"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ibcdatacsharp.UI.AngleGraph"
      xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AngleGraph">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <oxy:PlotView Grid.Row="0" x:Name="angleX" Model="{Binding modelX.PlotModel}"/>
        <oxy:PlotView Grid.Row="1" x:Name="angleY" Model="{Binding modelY.PlotModel}"/>
        <oxy:PlotView Grid.Row="2" x:Name="angleZ" Model="{Binding modelZ.PlotModel}"/>
    </Grid>
</Page>
